%REQUEST-PARAMETERS
     REP-DATE     PIC (X(8))     Text  Report Date :
%VARIABLES
 P      = '%' CONSTANT  
 RDATE1  = %REP-DATE% CONSTANT
 RDATE  = RDATE1 FORMAT (DATE) 
 EMPID  = EMPLOYEE-ID
 STAT   = EMP-STATUS OF MSF760  -
                WHERE    (EMPLOYEE-ID = EMPID)
 CATEG  = STAFF-CATEG     VIA (STAT)
 S      = GENDER          VIA (STAT)
 ETYP   = EMP-TYPE        VIA (STAT)
 HDATE  = HIRE-DATE       VIA (STAT)
 TDATE  = TERM-DATE       VIA (STAT)
 PU     = PERS-USER-FLD   VIA (STAT)
 PU02   = single (PU(2))
 PU03   = single (PU(3))
 PU04   = single (PU(4)) 
 PU05   = single (PU(5))
 PU06   = single (PU(6)) 
%*                                            
%*  Criteria for Selection:                   
%*     1.  Not a Worker's Comp                
%*     2.  Must be hired before the report date
%*     3.  Must be active during the report date                                     
 PRT    = CHOOSE ('N' IF (TDATE <= %REP-DATE%,  -
                          TDATE <> '00000000'), -
                  'N' IF (CATEG  = 'ZO'),       -
                  'N' IF (CATEG  = 'ZA'),       -
                  'N' IF (CATEG  = 'GC'),       -
                  'N' IF (ETYP   = 'WCP'),      - 
                  'N' IF (ETYP   = 'IWE'),      - 
                  'N' IF (HDATE  > %REP-DATE%), -  
                  'Y')                       
%*
%*****************************************************************************
%*               Staff Numbers By Employment Basis                           *
%*****************************************************************************
%*
%*   Determine Employment Level                         
%*      1 = Permanent Full-Time
%*      2 = Permanent Part-Time
%*      3 = Temporary Full-Time
%*      4 = Temporary Part-Time
%*      5 = SES
%*      6 = Casual
%*      7 = Other
%* 
 ASSRC  = ASSOC-REC OF MSF010 -
              WHERE (TABLE-TYPE = 'EMTY', -
                     TABLE-CODE = ETYP)
 ASSREC = ASSOC-REC  VIA (ASSRC)
 AREC   = substr(ASSREC,5,2)

 EVL    = CHOOSE ('1' IF (AREC = 'P1'), -
                  '2' IF (AREC = 'P2'), -
                  '3' IF (AREC = 'T1'), -
                  '4' IF (AREC = 'T2'), -
                  '5' IF (AREC = 'S1'), -
                  '6' IF (AREC = 'C1'), -
                  '7' IF (AREC = 'O1'), -
                  ' ')                                                  

%*                                          
%*   Employment Level Counts                     
%*                                          
 E1     = CHOOSE ( 1  IF (EVL = '1', -      
                          PRT = 'Y'), 0)    
 E2     = CHOOSE ( 1  IF (EVL = '2', -      
                          PRT = 'Y'), 0)    
 E3     = CHOOSE ( 1  IF (EVL = '3', -      
                          PRT = 'Y'), 0)    
 E4     = CHOOSE ( 1  IF (EVL = '4', -      
                          PRT = 'Y'), 0)    
 E5     = CHOOSE ( 1  IF (EVL = '5', -      
                          PRT = 'Y'), 0)    
 E6     = CHOOSE ( 1  IF (EVL = '6', -      
                          PRT = 'Y'), 0)    
 E7     = CHOOSE ( 1  IF (EVL = '7', -
                          PRT = 'Y'), 0)      
%*   Employment Level Totals and Grand Total   
 TEI1  = calc  (E1 * 1) I-PIC (9(5))
 TEI2  = calc  (E2 * 1) I-PIC (9(5))
 TEI3  = calc  (E3 * 1) I-PIC (9(5))
 TEI4  = calc  (E4 * 1) I-PIC (9(5))
 TEI5  = calc  (E5 * 1) I-PIC (9(5))
 TEI6  = calc  (E6 * 1) I-PIC (9(5))
 TEI7  = calc  (E7 * 1) I-PIC (9(5))

 TE1   = total (TEI1)  D-PIC (ZZ,ZZ9)     
 TE2   = total (TEI2)  D-PIC (ZZ,ZZ9)     
 TE3   = total (TEI3)  D-PIC (ZZ,ZZ9)     
 TE4   = total (TEI4)  D-PIC (ZZ,ZZ9)     
 TE5   = total (TEI5)  D-PIC (ZZ,ZZ9)     
 TE6   = total (TEI6)  D-PIC (ZZ,ZZ9)     
 TE7   = total (TEI7)  D-PIC (ZZ,ZZ9)     

%* ECNT   = CHOOSE (1   IF (PRT = 'Y'), 0)  
%* ETOT   = total (ECNT) D-PIC (ZZ,ZZ9)   

 TPA1  = calc (TE1+TE2) D-PIC (ZZ,ZZ9)
 TTA1  = calc (TE3+TE4) D-PIC (ZZ,ZZ9)
 ETOT   = calc (TE1+TE2+TE3+TE4+TE5+TE6+TE7) D-PIC (ZZ,ZZ9)
 ITOT   = calc (TE1 + TE2 + TE3 + TE4 + TE5 + TE6 + TE7) I-PIC (9(5))  

 TPB1  = calc (TE1+TE2) D-PIC (ZZ,ZZ9)
 TTB1  = calc (TE3+TE4) D-PIC (ZZ,ZZ9)

%*
%* TE11   = calc ((TE1 / ETOT) * 100)  I-PIC (9(3)V999)
 TE11   = calc ((TE1 / ITOT) * 100)  I-PIC (9(3)V999)
%* TE22   = calc ((TE2 / ETOT) * 100)  I-PIC (9(3)V999)
 TE22   = calc ((TE2 / ITOT) * 100)  I-PIC (9(3)V999)
%* TE33   = calc ((TE3 / ETOT) * 100)  I-PIC (9(3)V999)
 TE33   = calc ((TE3 / ITOT) * 100)  I-PIC (9(3)V999)
%* TE44   = calc ((TE4 / ETOT) * 100)  I-PIC (9(3)V999)
 TE44   = calc ((TE4 / ITOT) * 100)  I-PIC (9(3)V999)
%* TE55   = calc ((TE5 / ETOT) * 100)  I-PIC (9(3)V999)
 TE55   = calc ((TE5 / ITOT) * 100)  I-PIC (9(3)V999)
%* TE66   = calc ((TE6 / ETOT) * 100)  I-PIC (9(3)V999)
 TE66   = calc ((TE6 / ITOT) * 100)  I-PIC (9(3)V999)
%* TE77   = calc ((TE7 / ETOT) * 100)  I-PIC (9(3)V999)
 TE77   = calc ((TE7 / ITOT) * 100)  I-PIC (9(3)V999)

%* TTOT   = calc ((ETOT/ ETOT) * 100)  I-PIC (9(3)V999)
%*
 DE1    = TE11  D-PIC (ZZ9.999)
 DE2    = TE22  D-PIC (ZZ9.999)
 DE3    = TE33  D-PIC (ZZ9.999)
 DE4    = TE44  D-PIC (ZZ9.999)
 DE5    = TE55  D-PIC (ZZ9.999)
 DE6    = TE66  D-PIC (ZZ9.999)
 DE7    = TE77  D-PIC (ZZ9.999)
%* DTOT   = TTOT  D-PIC (ZZ9.999)

 TPC1  = calc (TE11 + TE22) D-PIC (ZZ9.999)
 TTC1  = calc (TE33 + TE44) D-PIC (ZZ9.999)
 DTC1  = calc (TE11 + TE22 + TE33 + TE44 + TE55 + TE66 + TE77) D-PIC (ZZ9.999)

%* 
%*   For Sub-Totals                           
%*
%*        P = Permanent                       
%*        T = Temporary                       
%*        F = Full-Time                       
%*        R = Part-Time                       
 PTVL   = CHOOSE ('P' IF (EVL >= '1',   -      
                          EVL <= '2'),  -      
                  'T' IF (EVL >= '3',   -      
                          EVL <= '4'), ' ')
 FRVL   = CHOOSE ('F' IF (EVL  = '1'),  -         
                  'F' IF (EVL  = '3'),  -         
                  'R' IF (EVL  = '2'),  -         
                  'R' IF (EVL  = '4'), ' ')
 P1     = CHOOSE ( 1  IF (PTVL = 'P', PRT = 'Y'), 0)        
 T      = CHOOSE ( 1  IF (PTVL = 'T', PRT = 'Y'), 0)        
 F      = CHOOSE ( 1  IF (FRVL = 'F', PRT = 'Y'), 0)        
 R      = CHOOSE ( 1  IF (FRVL = 'R', PRT = 'Y'), 0)        
 
 TPI    = calc  (P1 * 1) I-PIC (9(5))                                
 TTI    = calc  (T  * 1) I-PIC (9(5))                                
 TFI    = calc  (F  * 1) I-PIC (9(5))                                
 TRI    = calc  (R  * 1) I-PIC (9(5))                                

 TP     = total (P1) D-PIC (ZZ,ZZ9)                              
 TT     = total (T)  D-PIC (ZZ,ZZ9)                            
 TF     = total (F)  D-PIC (ZZ,ZZ9)                            
 TR     = total (R)  D-PIC (ZZ,ZZ9)                            
%*                                               
%*   Sex Counts                                  
%*                                               
 EM1    =  CHOOSE ( 1  IF (EVL   = '1', -        
                           S     = 'M', -        
                           PRT   = 'Y'), 0)      
 EF1    =  CHOOSE ( 1  IF (EVL   = '1', -        
                           S     = 'F', -        
                           PRT   = 'Y'), 0)      
 EM2    =  CHOOSE ( 1  IF (EVL   = '2', -        
                           S     = 'M', -        
                           PRT   = 'Y'), 0)      
 EF2    =  CHOOSE ( 1  IF (EVL   = '2', -        
                           S     = 'F', -        
                           PRT   = 'Y'), 0)      
 EM3    =  CHOOSE ( 1  IF (EVL   = '3', -        
                           S     = 'M', -        
                           PRT   = 'Y'), 0)      
 EF3    =  CHOOSE ( 1  IF (EVL   = '3', -        
                           S     = 'F', -        
                           PRT   = 'Y'), 0)     
 EM4    =  CHOOSE ( 1  IF (EVL   = '4', -            
                           S     = 'M', -            
                           PRT   = 'Y'), 0)          
 EF4    =  CHOOSE ( 1  IF (EVL   = '4', -            
                           S     = 'F', -            
                           PRT   = 'Y'), 0)          
 EM5    =  CHOOSE ( 1  IF (EVL   = '5', -            
                           S     = 'M', -            
                           PRT   = 'Y'), 0)          
 EF5    =  CHOOSE ( 1  IF (EVL   = '5', -            
                           S     = 'F', -            
                           PRT   = 'Y'), 0)          
 EM6    =  CHOOSE ( 1  IF (EVL   = '6', -            
                           S     = 'M', -            
                           PRT   = 'Y'), 0)          
 EF6    =  CHOOSE ( 1  IF (EVL   = '6', -            
                           S     = 'F', -            
                           PRT   = 'Y'), 0)          
 EM7    =  CHOOSE ( 1  IF (EVL   = '7', -            
                           S     = 'M', -            
                           PRT   = 'Y'), 0)         
 EF7    =  CHOOSE ( 1  IF (EVL   = '7', -  
                           S     = 'F', -    
                           PRT   = 'Y'), 0) 
%*   Sex Totals        
 TIM1   = calc  (EM1 * 1)  I-PIC (9(5))      
