       AUD    = AUDIT-TYPE
       CTYP   = choose ('Y' IF (%ITYP% = SPACES), -
                        'Y' IF (%ITYP% = 'A'), -
                        'Y' IF (%ITYP% = 'C', AUD = 'W'), -
                        'Y' IF (%ITYP% = 'D', AUD = 'D'), -
